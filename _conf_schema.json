{
    "session_based": {
        "description": "是否启用会话独立的情感系统",
        "type": "bool",
        "default": false,
        "hint": "开启后，每个群聊/私聊的情感状态将独立计算"
    },
    "favour_min": {
        "description": "好感度最小值",
        "type": "int",
        "default": -100,
        "hint": "设置好感度的最低值"
    },
    "favour_max": {
        "description": "好感度最大值",
        "type": "int",
        "default": 100,
        "hint": "设置好感度的最高值"
    },
    "change_min": {
        "description": "好感度单次变化最小值",
        "type": "int",
        "default": -10,
        "hint": "设置好感度单次减少的最大值"
    },
    "change_max": {
        "description": "好感度单次变化最大值",
        "type": "int",
        "default": 5,
        "hint": "设置好感度单次增加的最大值"
    },
    "admin_qq_list": {
        "description": "管理员QQ号列表",
        "type": "list",
        "default": [],
        "hint": "在此列表中的QQ号可以使用管理员命令"
    },
    "plugin_priority": {
        "description": "插件处理优先级",
        "type": "int",
        "default": 100000,
        "hint": "设置插件处理消息的优先级（数字越大越先执行，建议调高避免被其他插件干扰）"
    }
}